program: c1

def: press ahead
  // XXX: no top-level breathing should be done
  // XXX: we ought to ensure that collisions don't occur
  prim: 0, 1, none
  prim: 0, 1, none

def: press back
  // XXX: no top-level breathing should be done
  // XXX: we ought to ensure that collisions don't occur
  prim: 0, -1, none
  prim: 0, -1, none

def: concentric(c)
  spoken: concentric <c=anything>
  from: CENTER HALF
    call: _concentric([c], [c])
